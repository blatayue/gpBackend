{
  "version": 2,
  "builds": [
    {
      "src": "lib/*.js",
      "use": "@now/node"
    }
  ],
  "env": {
    "geniusApiKey": "@geniusapikey",
    "newrelicKey": "@newrelickey"
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "lib/index.js"
    }
  ],
  "alias": ["lyric.sparend.app"]
}
