{
  "name": "docker-koa-nextjs-graphql",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start:prod": "npm run build && npx micro ./lib/index.js",
    "build": "npx babel ./src -d ./lib --extensions \".ts\"",
    "deploy": "npm run build && now",
    "check-types": "tsc",
    "build:dev": "npx nodemon -i ./ --exec npx babel ./src -d ./lib -w ./src --source-maps inline --extensions \".ts\"",
    "start:dev": "npx nodemon -i ./ --exec npx -n --inspect micro-dev ./lib/index.js -i ./src/ -p 3001"
  },
  "babel": {
    "presets": [
      [
        "@babel/typescript"
      ],
      [
        "@babel/env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "@babel/proposal-class-properties",
      "@babel/proposal-object-rest-spread"
    ]
  },
  "quokka": {
    "babel": true
  },
  "author": "blatayue",
  "license": "ISC",
  "dependencies": {
    "@types/cheerio": "^0.22.10",
    "@types/ramda": "^0.25.47",
    "apollo-server": "^2.3.3",
    "apollo-server-micro": "^2.3.3",
    "axios": "^0.18.0",
    "cheerio": "^1.0.0-rc.2",
    "chroma-js": "^2.0.2",
    "dataloader": "^1.4.0",
    "get-image-colors": "^2.0.0",
    "get-pixels": "^3.3.2",
    "get-rgba-palette": "^2.0.1",
    "google-auth-library": "^3.1.0",
    "graphql": "^14.1.1",
    "graphql-tools": "^4.0.4",
    "imgix-core-js": "^1.2.0",
    "lancaster-stemmer": "^1.0.3",
    "micro": "^9.3.3",
    "micro-cors": "^0.1.1",
    "microrouter": "^3.1.3",
    "newrelic": "^5.3.0",
    "ramda": "^0.26.1",
    "sentiment": "^5.0.1",
    "stemmer": "^1.0.3",
    "stemr": "^1.0.0",
    "winston": "^3.2.1",
    "winston-papertrail": "^1.0.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-typescript": "^7.1.0",
    "@babel/register": "^7.0.0",
    "@types/chroma-js": "^1.4.1",
    "@types/graphql": "^14.0.5",
    "@types/imagemagick-native": "^1.7.29",
    "@types/micro-cors": "^0.1.0",
    "babel-loader": "^8.0.5",
    "micro-dev": "^3.0.0",
    "nodemon": "^1.18.9",
    "typescript": "^3.3.1"
  }
}
